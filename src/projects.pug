extends include/layout
block content
    .page.d-flex
        .slidebar.p-20.p-relative.bg-white 
            h3.slidebar__title.mt-0.p-relative.txt-c.fs-20 Future Face
            ul.slidebar__ulist
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="index.html") 
                        i.fa-solid.fa-chart-column.fa-fw
                        span Dashboard
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="settings.html") 
                        i.fa-solid.fa-gear.fa-fw
                        span Settings
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="profile.html") 
                        i.fa-regular.fa-user.fa-fw
                        span Profile
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="projects.html") 
                        i.fa-solid.fa-diagram-project.fa-fw
                        span Projects
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="courses.html") 
                        i.fa-solid.fa-graduation-cap.fa-fw
                        span Courses
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="friends.html") 
                        i.fa-solid.fa-user-group.fa-fw
                        span Friends
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="files.html") 
                        i.fa-regular.fa-file.fa-fw
                        span Files
                li.slidebar__item
                    a.slidebar__item--link.p-10.d-flex.align-center.active.fs-14.c-black.rad-6(href="plans.html") 
                        i.fa-solid.fa-table-columns.fa-fw
                        span Plans
        .content.w-full             
            //- start Header 
            .head.bg-white.p-15.between-flex
                .head__search.p-relative
                    input.p-10(type="search" placeholder="type a key")
                .head__icons.d-flex.align-center  
                    span.icons__notification.p-relative 
                        i.icons__notification--bell.fa-regular.fa-bell.fa-lg
                    img.icons__user-img(src="assets/images/avatar.png" alt="avatar")
            //- End Header 
            h1.page-title.p-relative  Projects 
            .projects-page.d-grid.m-20.gap-20
                .projects-page__project.bg-white.p-20.rad-6.p-relative 
                    span.projects-page__date.fs-13.c-grey 15/10/2021
                    h4.m-0 Saad Dashboard
                    p.c-grey.mt-10.mb-10.fs-14 Elzero Dashboard Project Design And Programming And Hosting
                    .projects-page__team
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-01.png", alt="")
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-02.png", alt="")
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-03.png", alt="")
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-04.png", alt="")
                    .projects-page__do.d-flex
                        span.fs-13.rad-6.bg-eee.btn-shape Programming
                        span.fs-13.rad-6.bg-eee.btn-shape Design
                        span.fs-13.rad-6.bg-eee.btn-shape Design
                        span.fs-13.rad-6.bg-eee.btn-shape Marketing
                    .projects-page__info.between-flex
                        .projects-page__info__prog.bg-eee
                            span.bg-red(style="width: 50%")
                        .projects-page__info__cost.fs-14.c-grey
                            i.fa-solid.fa-dollar-sign  2500
                
                .projects-page__project.bg-white.p-20.rad-6.p-relative 
                    span.projects-page__date.fs-13.c-grey 15/10/2021
                    h4.m-0 Saad Dashboard
                    p.c-grey.mt-10.mb-10.fs-14 Elzero Dashboard Project Design And Programming And Hosting
                    .projects-page__team
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-01.png", alt="")
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-02.png", alt="")
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-03.png", alt="")
                        a.projects-page__team__link(href="#") 
                            img(src="assets/images/team-04.png", alt="")
                    .projects-page__do.d-flex
                        span.fs-13.rad-6.bg-eee.btn-shape Programming
                        span.fs-13.rad-6.bg-eee.btn-shape Design
                        span.fs-13.rad-6.bg-eee.btn-shape Design
                        span.fs-13.rad-6.bg-eee.btn-shape Marketing
                    .projects-page__info.between-flex
                        .projects-page__info__prog.bg-eee
                            span.bg-red(style="width: 50%")
                        .projects-page__info__cost.fs-14.c-grey
                            i.fa-solid.fa-dollar-sign  2500
            
            